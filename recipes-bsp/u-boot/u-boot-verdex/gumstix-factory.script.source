echo "Booting from CF..."
set workedok Failed
if pinit on; then
       setenv bootargs console=ttyS0,115200n8 rootdelay=1 root=/dev/hda2
       if fatload ide 0 a2008000 uImage; then
               set workedok Success
       fi
       fatload ide 0 a2000000 uimage-verdex.dtb
fi
bootm a2008000 - 0xa2000000
