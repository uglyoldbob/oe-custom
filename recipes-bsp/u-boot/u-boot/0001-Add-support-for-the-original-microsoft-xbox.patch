diff --git a/arch/x86/Kconfig b/arch/x86/Kconfig
index a1c18d26e1..9495c7cb29 100644
--- a/arch/x86/Kconfig
+++ b/arch/x86/Kconfig
@@ -78,6 +78,9 @@ config VENDOR_GOOGLE
 config VENDOR_INTEL
 	bool "Intel"
 
+config VENDOR_MICROSOFT
