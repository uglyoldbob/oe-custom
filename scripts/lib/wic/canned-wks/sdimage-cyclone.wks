# short-description: Create SD card image for cyclone V soc
# long-description: Creates a partitioned SD card image for a cyclone V soc.

#--system-id=0xa2
part /PL --source rawcopy --sourceparams="file=whatever.bla" --ondisk mmcblk0 --fstype=vfat --label=pl --align 64 --size 16

part /boot --source bootimg-partition --ondisk mmcblk0 --fstype=vfat --label boot --active --align 64 --size 16

part / --source rootfs --ondisk mmcblk0 --fstype=ext4 --label root --align 8192 --size 1024

