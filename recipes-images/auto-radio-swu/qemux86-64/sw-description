{
	version = "@@PV@@";

	@@SWU_MACHINE_NAME@@ = {
		hardware-compatibility: [ "@@SWU_MACHINE_VERSION@@" ];

		images: (
			{
				filename = "auto-radio-@@MACHINE@@.rootfs.tar.gz.enc";
				type = "archive";
				compressed = "zlib";
				encrypted = true;
				device = "/dev/update";
				filesystem = "ext4";
